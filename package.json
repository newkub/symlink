{
  "name": "symlink-cli",
  "version": "0.0.1",
  "description": "CLI tool for creating symlinks with interactive prompts",
  "author": "Your Name",
  "private": false,
  "type": "module",
  "bin": {
    "symlink": "./src/index.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newkub/symlink.git"
  },
  "homepage": "https://github.com/newkub/symlink#readme",
  "keywords": [
    "symlink",
    "cli",
    "bun",
    "clack"
  ],
  "scripts": {
    "prerelease": "bun run verify",
    "release": "bun run build && release-it",
    "dev": "bun run src/index.ts",
    "build": "bun build ./src/index.ts --compile --outfile ./dist/symlink",
    "verify": "bun run lint && bun run test && bun run build",
    "lint": "tsc --noEmit && oxlint",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.4",
    "@types/bun": "latest",
    "@types/node": "^25.1.0",
    "@vitest/coverage-v8": "^4.0.18",
    "oxlint": "^1.42.0",
    "release-it": "^19.2.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0"
  }
}
